    </div>
   

    <br />
   <br />
    <div class="col-lg-4"></div>
    <div class="col-lg-6">
      <table>
          @using (Html.BeginForm("UpdateCustomerProfile1", "Customer", FormMethod.Post))
          {
            <tr><td>@Html.Label("USER I.D : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.Id, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("USERNAME : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.UserName, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("PASSWORD : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.Password, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("NAME : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.Name, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("CNIC : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.Cnic, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("COUNTRY : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.Country, new {placeholder="" })</td></tr>
            <tr><td>@Html.Label("CELL_NO : ", new { style = "color:white" })</td><td>@Html.TextBoxFor(m => m.CellNo, new {placeholder="" })</td></tr> 
            <tr><td></td><td> <br /><input type="submit" class="btn btn-primary col-lg-12 " value="UPDATE"/></td></tr>
           
          }
      </table>  
